<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>com.github.biconou</groupId>
  <artifactId>AudioPlayer</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>jar</packaging>

  <name>AudioPlayer</name>
  <url>http://maven.apache.org</url>

      <repositories>
        <repository>
            <id>local1</id>
            <name>Local Repository 1</name>
            <url>file://D:/AudioPlayer/repo</url>
        </repository>
      </repositories>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  
  <build>
  <plugins>
      <!-- ************************************************************************ -->
      <!-- plugin de compilation java : La version de java est 1.6. -->
      <!-- ************************************************************************ -->
      <!-- Plugin de compilation java -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
    </plugins>
  </build>

  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
 
    <dependency>
    	<groupId>org.kc7bfi</groupId>
    	<artifactId>jflac</artifactId>
    	<version>1.3-BICONOU</version>
    </dependency>
    <dependency>
    	<groupId>javazoom</groupId>
    	<artifactId>jl</artifactId>
    	<version>1.0.1-BICONOU</version>
    </dependency>
    <dependency>
    	<groupId>javazoom</groupId>
    	<artifactId>mp3spi</artifactId>
    	<version>1.9.5-BICONOU</version>
    </dependency>
    <dependency>
    	<groupId>org.tritonus</groupId>
    	<artifactId>tritonus_share</artifactId>
    	<version>BICONOU</version>
    </dependency>


    <dependency>
    	<groupId>org.slf4j</groupId>
    	<artifactId>slf4j-simple</artifactId>
    	<version>1.6.6</version>
    </dependency>
  </dependencies>
</project>
